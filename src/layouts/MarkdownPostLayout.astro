---
import BaseLayout from "./BaseLayout.astro";
const { frontmatter } = Astro.props;
---
<a href="/blog">Back to Blog Page</a>
<BaseLayout pageTitle={frontmatter.title}>
<p>Published on: {frontmatter.pubDate.slice(0,10)}</p>
<p><em>Description: {frontmatter.description}</em></p>
<p>Written by: {frontmatter.author}</p>
<img src={frontmatter.url} width="300" alt={frontmatter.image.alt}/>
<p>Tags: {frontmatter.tags.join(", ")}</p>
<slot />
</BaseLayout>